#include <iostream>

using namespace std;
int s[100];
int n, as, ev, k;
void tipar()
{
    for(int i=1; i<=n; i++)
        {
            cout<<s[i]<<" ";
        }
        cout<<endl;
}
int succesor()
{
    if(s[k]<n){
    s[k]++; return 1; }
   else return 0;
}
int solutie()
{
    return k==n;
}
int valid()
{
    for (int i=1; i<k; i++)
    {
        if(s[i]==s[k]) return 0;
    }
    return 1;
}

    void bt()
    {
         k=1;
        s[k]=0;
        while (k>0)
        {
            as=1; ev=0;
            while (as && !ev)
            {
                as=succesor();
                if(as)
                    ev=valid();}
                if (as)
                    if(solutie())
                    tipar();
                else {k++;
                    s[k]=0;
                }
                else k--; } }
int main()
{
    cout<<"valoare: ";
    cin>>n;
    bt();
    return 0;
}
