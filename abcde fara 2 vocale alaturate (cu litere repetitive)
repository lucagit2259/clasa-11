#include <iostream>
#include <cstring>
using namespace std;
int s[100], n,numara, l=4;
char lit[]=" abcde";
void tipar(int k)
{
    for (int i=1; i<=k; i++)
            cout<<lit[s[i]];
    cout<<endl;
}
int succesor(int k)
{
    if(s[k]<n){
        s[k]++; return 1;
    }
    else return 0;
}
int solutie(int k)
{
    return k==l;
}
int valid(int k)
{
    if(k>1){
        if(s[k]==1 && s[k-1]==1) return 0;
        if(s[k]==5 && s[k-1]==5) return 0;
        if(s[k]==5 && s[k-1]==1) return 0;
          if(s[k]==1 && s[k-1]==5) return 0;
        }
    return 1;
}
void bt(int k)
{
    int as,ev;
    k=1; s[k]=0;
    while (k>0)
    {
        as=1; ev=0;
        while(as && !ev)
        {
            as=succesor(k);
            if (as)
                ev=valid(k);
        }
        if(as)
            if(solutie(k))
           {
               numara++;
               cout<<"solutia nr "<<numara<<": ";
            tipar(k);
           }
        else{k++; s[k]=0; }
        else k--;
    }
}
int main()
{
    n=strlen(lit)-1;
    bt(n);
    cout<<endl<<numara<<" solutii";
    return 0;
}
