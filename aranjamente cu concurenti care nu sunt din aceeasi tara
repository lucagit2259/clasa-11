#include <iostream>
# include <fstream>
using namespace std;
int s[30];
string t[30];
int n, as, ev, k, l;
void tipar()
{
    for(int i=1; i<=k; i++)
        {
            cout<<s[i]<<" "<<t[s[i]]<<"   ";
        }
        cout<<endl;
}
int succesor()
{
    if(s[k]<n){
    s[k]++; return 1; }
   else return 0;
}
int solutie()
{
    return k==l;
}
int valid()
{
    for (int i=1; i<k; i++)
    {
        if(s[i]==s[k]) return 0;
        if (t[s[i]]==t[s[k]])return 0;
    }
    return 1;
}

    void bt()
    {
         k=1;
        s[k]=0;
        while (k>0)
        {
            as=1; ev=0;
            while (as && !ev)
            {
                as=succesor();
                if(as)
                    ev=valid();}
                if (as)
                    if(solutie())
                    tipar();
                else {k++;
                    s[k]=0;
                }
                else k--; } }
int main()
{
    ifstream fin("fisier.txt");
    cout<<"cati concurenti joaca astazi in grupe de cate cat? ";
    cin>>n>>l;
    for (int i = 1; i <= n; i++)
    {
        fin>>t[i];
    }
    
    bt();
    cout<<endl<<endl;
    for (int i = 1; i <= n; i++)
    {
        cout<<t[i]<<" ";
    }
    
    return 0;
}
