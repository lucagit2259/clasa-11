#include <iostream>
#include <cmath>
using namespace std;
int s[100], k, n, as, ev, o;
void tipar()
{
    for(int i=1; i<=n; i++)
    {
        for(int j=1; j<=k; j++)
        {
        if(s[i]==j)
        cout<<"| @ |";
        else cout<<"| # |";
        }
        cout<<endl<<endl;
    }
cout<<endl;
}
int succesor()
{
    if(s[k]<n){
        s[k]++; return 1;
    }
    return 0;
}
int solutie()
{
    return k==n;
}
int valid()
{
    for(int i=1; i<k; i++)
        if(abs(s[k]-s[i])==abs(k-i) || s[k]==s[i]) return 0;
    return 1;
}
void bt()
{
    k=1;
    s[k]=0;
    while(k>0)
    {
        as=1; ev=0;
        while(as&& !ev)
        {
            as=succesor();
            if(as);
            ev=valid(); }
        if(as)
            if(solutie())
            {
                tipar(); o++;
            }
            else{
                k++;
                s[k]=0;
            }
            else k--;
    }
}
int main()
{
    cout<<" ";
    cin>>n;
    bt();
    cout<<endl<<o<<" solutii";
    return 0;
}
