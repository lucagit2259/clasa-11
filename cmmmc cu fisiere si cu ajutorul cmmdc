 #include <iostream>
 #include <cmath>
 #include <fstream>
using namespace std;
ifstream fin ("nr.in");
ofstream fout ("nr.out");
int cmmdc(int x, int y)
{
    while (x!=y)
    {
        if (x>y)
        {
            x=x-y;
        }
        else y=y-x;   
    }
    return x;
}
void cmmmc(int a, int b, int &q)
{
    q= a*b/cmmdc(a, b);

}

int main()
{
    int a, b, q;
    while (fin>>a>>b)
    {
        cmmmc(a, b, q);
        fout<<q<<endl;
        cout<<q<<endl;
    }
    fin.close();
    fout.close();
    return 0;
}
/* fisierul nr.in
  12 14
11 12
4 8
  */
/* in fisierul nr.out trebuie sa apara rezultatele */
